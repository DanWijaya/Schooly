{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/Web App/client"
        },

    
    {
        "command": "npm run dev",
        "name": "Run Schooly",
        "request": "launch",
        "type": "node-terminal",
        "localRoot": "${workspaceFolder}/Web App/client"
    },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/Web App/client/src/components/misc/nav-bar/NavBarLoggedInContents.js"
        }
    ]
}